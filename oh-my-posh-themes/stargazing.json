{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "yellow",
          "properties": {
            "root_icon": "(Admin) ",
            "template": "\uf0e7"
          },
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "blue",
          "properties": {
            "style": "full",
            "template": "{{ .Path }}"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#FBB829",
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "branch_icon": "î‚  ",
            "template": " {{ .HEAD }} "
          }
        },
        {
          "foreground": "lightBlue",
          "properties": {
            "template": " {{ .CurrentDate | date .Format }} ",
            "time_format": "ðŸ•™ 15:04:05"
          },
          "style": "plain",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "foreground": "#FCE8C3",
          "properties": {
            "always_enabled": true,
            "display_exit_code": true,
            "template": "\u276f{{ if gt .Code 0 }}{{ .Meaning }}{{ end }} "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "foreground": "cyan",
          "properties": {
            "style": "austin",
            "template": " \u23f2\ufe0f {{ .FormattedMs }} ",
            "threshold": 500
          },
          "style": "plain",
          "type": "executiontime"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}}(Admin){{end}} {{.PWD}}",
  "version": 1
}
